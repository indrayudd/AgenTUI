{
  "name": "agentui",
  "version": "0.1.0",
  "description": "Terminal chat TUI powered by LangChain deep agents",
  "type": "module",
  "main": "dist/src/cli.js",
  "bin": {
    "agentui": "./bin/agentui.cjs"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx src/cli.tsx",
    "agent": "tsx src/agent-cli.ts",
    "build": "tsc -p tsconfig.json && node scripts/copy-assets.mjs",
    "start": "node dist/src/cli.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run",
    "bin:set": "node scripts/set-command-prefix.mjs",
    "notebook:test": "tsx scripts/notebook-smoketest.ts",
    "agent:smoke": "tsx scripts/agent-smoketest.ts",
    "agent:testfs": "tsx scripts/test-filesystem.ts",
    "test:agentui-bin": "node scripts/test-agentui-bin.mjs",
    "test:watch": "vitest"
  },
  "keywords": [
    "tui",
    "langchain",
    "ink",
    "cli"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@langchain/openai": "^1.1.1",
    "deepagents": "^1.1.0",
    "dotenv": "^17.2.3",
    "ink": "^6.5.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "langchain": "^1.0.4",
    "react": "^19.2.0",
    "string-width": "^8.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "ink-testing-library": "^4.0.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9"
  }
}
